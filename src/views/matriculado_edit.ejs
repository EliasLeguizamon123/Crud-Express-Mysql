<% include partials/_header %>

<div class="container mt-5">
  <div class="row">
    <div class="col-md-5 offset-md-3 card">
      <div class="card-body">
        <form action="/update/<%= data.nro_matricula %>" method="POST">
          <div class="form-group">
            <input type="text" name="nro_matricula" value="<%= data.nro_matricula%>" placeholder="nro_matricula" class="form-control" autofocus>
          </div>
          <div class="form-group">
            <input type="text" name="nombre" value="<%= data.nombre%>" placeholder="nombre" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="apellido" value="<%= data.apellido%>" placeholder="apellido" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="dni" value="<%= data.dni%>" placeholder="dni" class="form-control">
          </div>
          <button type="submit" class="btn btn-primary">
            Editar Matriculado
          </button>
        </form>
      </div>
    </div>
  </div>
</div>

<% include partials/_footer %>
